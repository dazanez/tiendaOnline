<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="vista.MainController"
      alignment="CENTER" spacing="20"
      prefWidth="400" prefHeight="300">
    
    <padding>
        <Insets top="30" right="30" bottom="30" left="30"/>
    </padding>

    <Button text="Ejecutar Proxy" onAction="#ejecutarProxy" prefWidth="200"/>
    <Button text="Mostrar Facade" onAction="#mostrarFacade" prefWidth="200"/>
    <Button text="Mostrar Flyweight" onAction="#mostrarFlyweight" prefWidth="200"/>
    
</VBox>
