<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="224.0" prefWidth="293.0" spacing="10" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controlador.ProxyController">
    
    <padding>
        <Insets bottom="20" left="20" right="20" top="20" />
    </padding>

    <Label text="Ingrese el rol del usuario" />
    <TextField fx:id="txtRol" promptText="Ej: admin o user" />
    <Button onAction="#mostrarDetalles" text="Mostrar detalles del producto" />
    <Label fx:id="lblResultado" text="" wrapText="true" />
</VBox>
