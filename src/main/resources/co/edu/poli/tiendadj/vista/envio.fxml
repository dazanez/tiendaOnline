<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.poli.tiendadj.controlador.EnvioController">
   <children>
   
       <Label layoutX="120.0" layoutY="20.0" text="Seleccione tipo de envío:" />
       <ComboBox fx:id="cbEnvio" layoutX="120.0" layoutY="40.0" prefWidth="160.0" />
   
       <Label layoutX="120.0" layoutY="80.0" text="Seleccione tipo de mercancía:" />
       <ComboBox fx:id="cbMercancia" layoutX="120.0" layoutY="100.0" prefWidth="160.0" />
   
       <Button layoutX="140.0" layoutY="140.0" onAction="#procesarEnvio" text="Procesar Envío" />
   
       <Label fx:id="lblResultado" layoutX="100.0" layoutY="180.0" maxWidth="250.0" text="" wrapText="true" />
      <Button layoutX="22.0" layoutY="248.0" mnemonicParsing="false" onAction="#openMenu" prefHeight="30.0" prefWidth="69.0" text="Menu" textAlignment="CENTER">
         <font>
            <Font name="System Bold Italic" size="15.0" />
         </font>
      </Button>
   </children>

</AnchorPane>
